{
  "name": "austa-cockpit-backend",
  "version": "1.0.0",
  "description": "Backend services for AUSTA Cockpit Analytics",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "build:skip-types": "echo 'Skipping strict TypeScript checking' && mkdir -p dist && cp -r src/* dist/ && echo 'Build completed with warnings but files copied'",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "db:setup": "prisma generate && prisma db push",
    "db:reset": "prisma migrate reset --force"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^9.0.2",
    "@opentelemetry/exporter-jaeger": "^2.0.1",
    "@opentelemetry/instrumentation": "^0.202.0",
    "@opentelemetry/instrumentation-express": "^0.51.0",
    "@opentelemetry/instrumentation-http": "^0.202.0",
    "@opentelemetry/instrumentation-pg": "^0.54.0",
    "@opentelemetry/instrumentation-redis": "^0.49.1",
    "@opentelemetry/sdk-node": "^0.202.0",
    "@opentelemetry/sdk-trace-base": "^2.0.1",
    "@opentelemetry/sdk-trace-node": "^2.0.1",
    "@prisma/client": "^6.10.1",
    "@prisma/engines": "^6.10.1",
    "@types/bull": "^3.15.9",
    "@types/swagger-jsdoc": "^6.0.4",
    "argon2": "^0.31.2",
    "axios": "^1.7.10",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.3",
    "openid-client": "^5.7.1",
    "prom-client": "^15.1.3",
    "redis": "^4.6.12",
    "speakeasy": "^2.0.0",
    "swagger-jsdoc": "^6.2.8",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-elasticsearch": "^0.19.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-validator": "^2.20.33",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/speakeasy": "^2.0.10",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prisma": "^6.10.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
